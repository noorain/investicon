<script type="text/javascript">
	var button = $('#click-this');
	var box = $('#search-box');
	var closeBoxButton = $('#close-button');
	var body1 = $('body');
	
	button.click(function(event){
		event.stopPropagation();
		box.show();

		box.click(function(e){
			e.stopPropagation();
		});
		closeBoxButton.one('click', function(){
			box.hide();
		});
		body1.one('click', function(){
			box.hide();
		});
	});
	
</script>
